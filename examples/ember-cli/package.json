{
  "name": "ember-example",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "now-build": "node ../../scripts/bootstrap --core && yarn run build-storybook --quiet",
    "build": "ember build",
    "build-storybook": "yarn build && cp -r public/* dist && build-storybook -s dist",
    "dev": "ember serve",
    "storybook": "yarn build && start-storybook -p 9009 -s dist",
    "storybook:dev": "yarn dev & start-storybook -p 9009 -s dist"
  },
  "dependencies": {
    "ember-template-compiler": "^1.9.0-alpha"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-a11y": "5.0.0",
    "@storybook/addon-actions": "5.0.0",
    "@storybook/addon-backgrounds": "5.0.0",
    "@storybook/addon-centered": "5.0.0",
    "@storybook/addon-knobs": "5.0.0",
    "@storybook/addon-links": "5.0.0",
    "@storybook/addon-notes": "5.0.0",
    "@storybook/addon-options": "5.0.0",
    "@storybook/addon-storysource": "5.0.0",
    "@storybook/addon-viewport": "5.0.0",
    "@storybook/addons": "5.0.0",
    "@storybook/ember": "5.0.0",
    "babel-loader": "^8",
    "broccoli-asset-rev": "^3.0.0",
    "cross-env": "^5.2.0",
    "ember-ajax": "^4.0.2",
    "ember-cli": "~3.7.1",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^7.5.0",
