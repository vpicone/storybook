    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-storybook": "^0.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-resolver": "^5.1.3",
    "ember-source": "~3.8.0",
    "loader.js": "^4.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
